{
    "env": {
        "es6": true,
        "node": true,
        "mocha": true
    },
    "extends": "airbnb-base",
    "rules": {
        "indent": [
            "error",
            4
        ],
        "linebreak-style": 0,
        "no-underscore-dangle": [
            "error",
            {
                "allowAfterThis": true
            }
        ]
    }
}